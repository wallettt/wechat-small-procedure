/* pages/setting/setting.wxss */
.wode_item {
  height: 45px;
border-bottom: 1px solid #E5E5E5;
padding: 0 10px;
line-height: 45px;
position: relative;
}
.arrow_wrap {
  position: absolute;
  width: 50px;
  height: 45px;
  right: 0;
  top: 0;
  background-image: url(../../image/icon-arrow.png);
  background-repeat: no-repeat;
  background-size: 16px;
  background-position: 17px 14px;
}